@{
  Layout = "_Layout";
}
@using PierreTracker.Models;
@if (@Model.Count == 0)
{
  <p>You do not have any vendors currently.</p>
}
<section class="flex flex-col items-start justify-center p-8 border border-slate-500 rounded-xl gap-3 divide-y divide-dashed divide-slate-500">
    @foreach (Vendor vendor in @Model)
    {
      <div><a class="text-2xl" href="/vendors/@vendor.Id">@vendor.Name</a></div>
    }
</section>